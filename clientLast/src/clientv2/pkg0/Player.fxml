            <HBox alignment="CENTER" prefHeight="40.0" prefWidth="600.0">
               <children>
                  <Button fx:id="btnOpenFile" alignment="CENTER" mnemonicParsing="false" onAction="#onClickOpen" prefHeight="25.0" prefWidth="71.0" text="Open File">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btnPlay" mnemonicParsing="false" onAction="#onClickPlay" prefHeight="25.0" prefWidth="44.0" text="Play">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btnPause" mnemonicParsing="false" onAction="#onClickBtnPause" prefHeight="25.0" prefWidth="49.0" text="Pause">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btnStop" mnemonicParsing="false" onAction="#onClickStop" prefHeight="25.0" prefWidth="42.0" text="Stop">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btnSlower" mnemonicParsing="false" onAction="#onClickBtnSlower" text="&lt;&lt;&lt;">
