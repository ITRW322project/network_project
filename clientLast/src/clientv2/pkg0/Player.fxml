                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btnSlow" mnemonicParsing="false" onAction="#onClickBtnSlow" text="&lt;&lt;">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btnFastForward" mnemonicParsing="false" onAction="#onClickFastForward" text="&gt;&gt;">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btnFastFastForward" mnemonicParsing="false" onAction="#onClickFastFastForward" text="&gt;&gt;&gt;">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btnExit" mnemonicParsing="false" onAction="#exit" prefHeight="25.0" prefWidth="37.0" text="Exit">
